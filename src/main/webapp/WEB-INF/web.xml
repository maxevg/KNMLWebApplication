<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                      http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
  <display-name>WebApp of KNML</display-name>

  <welcome-file-list>
    <welcome-file>main.jsp</welcome-file>
    <welcome-file>Rectangle.jsp</welcome-file>
    <welcome-file>Trapezoid.jsp</welcome-file>
    <welcome-file>Simpson.jsp</welcome-file>
    <welcome-file>Gauss.jsp</welcome-file>
    <welcome-file>Thomas.jsp</welcome-file>
    <welcome-file>Jacobi.jsp</welcome-file>
    <welcome-file>Zeidel.jsp</welcome-file>
    <welcome-file>Eigen.jsp</welcome-file>
    <welcome-file>QR.jsp</welcome-file>
    <welcome-file>LU.jsp</welcome-file>
    <welcome-file>SVD.jsp</welcome-file>
  </welcome-file-list>

  <servlet>
    <servlet-name>Main</servlet-name>
    <servlet-class>servlets.MainServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>Start</servlet-name>
    <servlet-class>servlets.StartServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>Gauss</servlet-name>
    <servlet-class>servlets.GaussServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>Thomas</servlet-name>
    <servlet-class>servlets.ThomasServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>Jacobi</servlet-name>
    <servlet-class>servlets.JacobiServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>Zeidel</servlet-name>
    <servlet-class>servlets.ZeidelServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>Rectangle</servlet-name>
    <servlet-class>servlets.RectangleServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>Trapezoid</servlet-name>
    <servlet-class>servlets.TrapezoidServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>Simpson</servlet-name>
    <servlet-class>servlets.SimpsonServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>Eigen</servlet-name>
    <servlet-class>servlets.EigenServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>QR</servlet-name>
    <servlet-class>servlets.QRServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>LU</servlet-name>
    <servlet-class>servlets.LUServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>SVD</servlet-name>
    <servlet-class>servlets.SVDServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>Start</servlet-name>
    <url-pattern></url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>Main</servlet-name>
    <url-pattern>/main</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>Rectangle</servlet-name>
    <url-pattern>/Rectangle</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>Trapezoid</servlet-name>
    <url-pattern>/Trapezoid</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>Simpson</servlet-name>
    <url-pattern>/Simpson</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>Gauss</servlet-name>
    <url-pattern>/Gauss</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>Thomas</servlet-name>
    <url-pattern>/Thomas</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>Jacobi</servlet-name>
    <url-pattern>/Jacobi</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>Zeidel</servlet-name>
    <url-pattern>/Zeidel</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>Eigen</servlet-name>
